<!DOCTYPE html>
<html lang="en">
    <%- include('../head.ejs') %>
    <body>
        <header></header>
        <%- include('../menu.ejs') %>

        <main class="content-border">
            <%- include('../navigation.ejs') %>

            <div class="content">
                <div class="left">
                    <h2>Daily / Weekly Achievements</h2>

                    <table class="content-border">
                        <colgroup>
                            <col width="*">
                            <col width="150px">
                        </colgroup>
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Unlocked on</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% for (let achievement of achievementRepeatable) { %>
                                <tr>
                                    <td><%= achievementTitles[achievement.type] %></td>
                                    <td><%= formatDate(achievement.time_last_level, 'day-only') %></td>
                                </tr>
                            <% } %>
                        </tbody>
                    </table>
                </div>
            </div>
        </main>

        <%- include('../footer.ejs') %>

        <script type="text/javascript">
            const {
                marketId,
                worldNumber
            } = <%- JSON.stringify(exportValues) %>
        </script>
    </body>
</html>
